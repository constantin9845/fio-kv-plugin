[global]

ioengine=./unvme2_fio_plugin
json_path=./unvme2_config.json

# bs is actually not used 
# BOTH MUST MATCH VALUES IN [LOAD - WARMUP - RUN fio jobs]
size=10G
bs=4k

thread=1

direct=1
group_reporting=1

filename=0000.00.05.0
exitall_on_error=1

ks=16
kd=0.1

variable_value_size=1

ratio_512=70
ratio_1kb=15
ratio_2kb=10
ratio_3kb=4
ratio_4kb=1


# LOAD
[load]
stonewall
rw=write
iodepth=64
numjobs=1

